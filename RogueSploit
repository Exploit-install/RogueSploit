		pkill airbase-ng
		service network-manager restart
		service hostapd restart
		sleep 1
		echo -e $yellow"--<[*] Hope you pwned someone today! [*]>--"
		echo -e $yellow"--<[*] Thank You For Using RogueSploit B) [*]>--"
		sleep 2
		clear
		exit
	elif [ $back != 'y' ] && [ $back != 'Y' ] && [ $back != 'Yes' ]
		then
		menu
	fi
fi
