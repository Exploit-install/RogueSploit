	sleep 2
	service network-manager stop
	echo "interface=$wlan
driver=nl80211
ssid=$nameap
channel=6 "> $PWD/hostapd.conf
	pkill hostapd
	rogueon && xterm -title "BeEF" -bg "#000000" -fg "#ffff00" -geometry 100x25-1-1 -e "hostapd hostapd.conf"
	service network-manager start
	menu
